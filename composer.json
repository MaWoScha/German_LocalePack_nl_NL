{
  "name": "mawoscha/german_localepack_nl_nl",
  "type": "magento-module",
  "license":"OSL-3.0",
  "description":"Dutch (Netherlands) Community Translation",
  "authors":[
    {
      "name":"Paul Hachmang",
      "email":"paul@h-o.nl"
    },
    {
      "name":"MaWoScha",
      "email":"manfredwormschaefer@gmail.com"
    }
  ],
  "require": {
    "magento-hackathon/magento-composer-installer": "*"
  },
  "extra": {
    "magento-root-dir": "./",
    "magento-deploystrategy": "copy",
    "map": [
      ["./app/code/community/German/LocalePackNl", "./app/code/community/German/LocalePackNl"],
      ["./app/etc/modules/German_LocalePackNl.xml", "./app/etc/modules/German_LocalePackNl.xml"],
      ["./downloader/template_nl_1.5", "./downloader/template_nl_1.5"],
      ["./downloader/template_nl_1.7", "./downloader/template_nl_1.7"],
      ["./app/locale/de_DE/German_LocaleNl.csv", "./app/locale/de_DE/German_LocaleNl.csv"],
      ["./app/locale/en_US/German_LocaleNl.csv", "./app/locale/en_US/German_LocaleNl.csv"],
      ["./app/locale/nl_NL/German_LocaleNl.csv", "./app/locale/nl_NL/German_LocaleNl.csv"]
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
